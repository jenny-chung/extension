{
    "manifest_version": 3,
    "name": "water you doing?",
    "version": "1.0.0",
    "description": "An extension that reminds you to take breaks and stay hydrated!",
    "author": "Jenny Chung",
    "permissions": ["notifications", "alarms", "storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    }
    
}